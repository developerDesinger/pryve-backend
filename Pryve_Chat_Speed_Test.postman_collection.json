									"const totalTime = endTime - startTime;",
									"",
									"if (pm.response.code === 200 || pm.response.code === 201) {",
									"    const response = pm.response.json();",
									"    const aiResponse = response.data.aiResponse;",
									"    ",
									"    console.log('üöÄ SIMPLE MESSAGE TEST RESULTS:');",
									"    console.log('=' .repeat(50));",
									"    console.log('‚è±Ô∏è  Total Time:', totalTime + 'ms');",
									"    console.log('ü§ñ AI Processing:', (aiResponse?.processingTime || 0) + 'ms');",
									"    console.log('üß† Model Used:', aiResponse?.aiModel || 'unknown');",
									"    console.log('üî¢ Tokens Used:', aiResponse?.tokensUsed || 0);",
									"    console.log('üìä Network + DB:', (totalTime - (aiResponse?.processingTime || 0)) + 'ms');",
									"    ",
									"    // Check optimization",
									"    if (aiResponse?.aiModel === 'gpt-4o-mini') {",
									"        console.log('‚úÖ OPTIMIZATION SUCCESS: Using fast model!');",
									"    } else {",
